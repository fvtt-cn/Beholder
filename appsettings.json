{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Spectator": {
    "Directory": ".",
    "TrimStart": "",
    "PreloadEnabled": true,
    "ExcludeExtensions": [],
    "ExcludePaths": [],
    "OnCreated": true,
    "OnChanged": true,
    "OnDeleted": true,
    "OnRenamed": true
  },
  "Thief": {
    "CheckInterval": 30,
    "RefreshTakeCount": 1000,
    "PreloadTakeCount": 200
  },
  "Handler": {
    "Method": "ConsoleLog",
    "ConsoleLog": {
      "Prefix": ""
    },
    "AliYunCdn": {
      "Prefix": "my-foundryvtt.net/",
      "RegionId": "cn-hangzhou",
      "AccessKeyId": "<my-aliyun-accesskey-id>",
      "Secret": "<my-aliyun-accesskey-secret>"
    }
  },
  "Pruning": {
    "Method": "SimplePruning",
    "SimplePruning": {
      "MergeRequirement": 25,
      "PreloadCheckRoot": "./"
    }
  }
}
